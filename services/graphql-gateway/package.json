{
  "name": "graphql-gateway",
  "version": "1.0.0",
  "description": "GraphQL Gateway for Platyplus",
  "scripts": {
    "start": "node index.js",
    "dev-docker": "../node_modules/nodemon/bin/nodemon.js index.js --inspect=5858",
    "dev-host": "nodemon index.js --inspect=5858",
    "start-watch": "nodemon index.js --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon index.js --inspect-brk=0.0.0.0:9229",
    "test": "cross-env NODE_ENV=test PORT=8081 mocha --timeout 10000 --exit --inspect=0.0.0.0:9230",
    "test-watch": "nodemon --exec \"npm test\"",
    "test-wait-debuger": "cross-env NODE_ENV=test PORT=8081 mocha --no-timeouts --exit --inspect-brk=0.0.0.0:9230"
  },
  "keywords": [
    "hasura",
    "fastify",
    "graphql"
  ],
  "author": "Pierre-Louis Mercereau",
  "license": "MIT",
  "dependencies": {
    "apollo-link-context": "^1.0.14",
    "apollo-link-http": "^1.5.11",
    "apollo-server-fastify": "^2.4.8",
    "fastify": "^2.0.0",
    "fastify-healthcheck": "^2.0.0",
    "graphql": "^14.1.1",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
