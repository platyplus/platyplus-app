{
  "name": "platyplus-authentication",
  "version": "1.0.0",
  "description": "Auth server for Platyplus",
  "scripts": {
    "start": "node bin/www",
    "dev-docker": "../node_modules/nodemon/bin/nodemon.js bin/www --inspect=5858",
    "dev-host": "nodemon bin/www --inspect=5858",
    "start-watch": "nodemon bin/www --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon bin/www --inspect-brk=0.0.0.0:9229",
    "test": "cross-env NODE_ENV=test PORT=8081 mocha --timeout 10000 --exit --inspect=0.0.0.0:9230",
    "test-watch": "nodemon --exec \"npm test\"",
    "test-wait-debuger": "cross-env NODE_ENV=test PORT=8081 mocha --no-timeouts --exit --inspect-brk=0.0.0.0:9230"
  },
  "keywords": [
    "hasura",
    "auth",
    "graphql"
  ],
  "author": "Pierre-Louis Mercereau",
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "^4.1.0",
    "axios": "^0.18.0",
    "graphql": "^14.2.1",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.1.4",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.10"
  }
}
